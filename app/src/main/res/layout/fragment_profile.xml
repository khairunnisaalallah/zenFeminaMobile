        android:text="Log Out"
        android:textColor="#FFFFFF"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/btntext4"
        android:layout_width="wrap_content"
